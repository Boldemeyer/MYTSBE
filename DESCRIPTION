  coda(>= 0.18-1),
  lattice(>= 0.20-33),
  superdiag(>= 1.1),
  mcmcplots(>= 0.4.2),
  ggmcmc(>= 0.8),
  data.table(>= 1.9.6),
  useful(>= 1.2.0)
RoxygenNote: 5.0.1
